[
  {
    "name": "virtual-kubelet",
    "image": "781416470409.dkr.ecr.eu-west-1.amazonaws.com/virtual-kubelet:latest",
    "cpu": 256,
    "memory": 512,
    "essential": true,
    "environment": [
      {
        "name": "KUBELET_PORT",
        "value": "10250"
      },
      {
        "name": "EKS_CLUSTER_NAME",
        "value": "lab-eks"
      },
      {
        "name": "FARGATE_CLUSTER_REGION",
        "value": "eu-west-1"
      },
      {
        "name": "FARGATE_CLUSTER_NAME",
        "value": "lab-ecs"
      },
      {
        "name": "FARGATE_SUBNETS",
        "value": "\"subnet-0786e96133704484c\",\"subnet-01cb131e62c1bbb87\""
      },
      {
        "name": "FARGATE_SGS",
        "value": "\"sg-0ab98ba7f76309903\""
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/aws/ecs/lab-ecs/task",
        "awslogs-region": "eu-west-1",
        "awslogs-stream-prefix": "virtual-kubelet"
      }
    }
  }
]